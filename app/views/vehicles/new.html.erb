
<div class="container">
  <%= simple_form_for [current_user, @vehicle] do |f| %>
    <%= f.input :make %>
    <%= f.input :model %>
    <%= f.input :year, as: :date, html5: true %>
    <%= f.input :passengers, as: :integer %>
    <%= f.input :description, as: :text %>
    <%= f.input :price, as: :float %>
    <%= f.input :city %>
    <%= f.input :country %>
    <%= f.input :photos, as: :file, input_html: { multiple: true } %>
    <%= f.submit "Add car" %>
  <% end %>
</div>
